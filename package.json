{
	"name": "re-muve",
	"version": "0.1.0",
	"description": "re|muve - a micro view builder",
	"main": "index.js",
	"scripts": {
		"test": "ava -v -w",
		"test:coverage": "nyc ava"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/fwilkerson/re-muve.git"
	},
	"keywords": [
		"jsx",
		"dom",
		"vdom"
	],
	"author": "Frank A. Wilkerson",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/fwilkerson/re-muve/issues"
	},
	"homepage": "https://github.com/fwilkerson/re-muve#readme",
	"devDependencies": {
		"@babel/core": "^7.0.0-beta.40",
		"@babel/preset-env": "^7.0.0-beta.40",
		"@babel/register": "^7.0.0-beta.40",
		"ava": "^1.0.0-beta.3",
		"browser-env": "^3.2.5",
		"nyc": "^11.4.1",
		"prettier": "^1.10.2",
		"xo": "^0.20.3"
	},
	"ava": {
		"require": [
			"@babel/register"
		]
